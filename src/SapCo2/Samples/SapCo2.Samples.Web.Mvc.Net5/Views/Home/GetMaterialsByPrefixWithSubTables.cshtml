@model IEnumerable<SapCo2.Samples.Core.Models.Material>

@{
    ViewData["Title"] = "GetMaterialsByPrefixWithSubTables";
}
<div>
    <a asp-action="Index"><i class="arrow"></i> Menüye Dön</a>
</div>
<h1>GetMaterialsByPrefixWithSubTables</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Code)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Definition.Definition)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Definition.Definition)
            </td>
        </tr>
}
    </tbody>
</table>
